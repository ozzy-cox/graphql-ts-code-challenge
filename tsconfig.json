{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      
      "es6"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*",
        "src/*.ts"
      ],
    },
    "allowJs": true,
    "outDir": "build",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
  }
}